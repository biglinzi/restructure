{{#extend "css"}}
{{#content "sprites" mode="replace"}}
.icon {
  display: inline-block;
  background-repeat: no-repeat;
}
{{#each sprites}}
{{{name}}} {
  background-image: url({{{escaped_image}}});
  background-position: {{px.offset_x}} {{px.offset_y}};
  width: {{px.width}};
  height: {{px.height}};
}
{{/each}}
{{/content}}
{{/extend}}